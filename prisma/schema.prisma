// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User{
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  orders    Order[]
}
model Order{
  id        Int      @id @default(autoincrement())
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
  token_amount Int
  price_amount Int
}